{
  "name": "common-exports",
  "version": "1.3.5",
  "description": "Convert ES Module to CommonJS.",
  "main": "dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    }
  },
  "scripts": {
    "build": "gulp build",
    "htmldocs": "jsdoc -R MAIN.md -c ./.jsdoc.conf.js -d docs",
    "readme": "gulp readme",
    "test": "gulp testFull"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jheagle/common-exports.git"
  },
  "keywords": [
    "CommonJS",
    "ES6",
    "Modules",
    "bundle",
    "build"
  ],
  "author": "Joshua Heagle",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jheagle/common-exports/issues"
  },
  "homepage": "https://github.com/jheagle/common-exports#readme",
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-throw-expressions": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime-corejs3": "^7.28.4",
    "core-js": "^3.45.1",
    "fs": "^0.0.1-security",
    "gulp": "^5.0.1",
    "gulp-babel": "^8.0.0",
    "regenerator-runtime": "^0.14.1",
    "test-filesystem": "^1.3.9",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.17",
    "@types/gulp-babel": "^6.1.33",
    "@types/through2": "^2.0.41",
    "babel-jest": "^30.1.2",
    "browserify": "^17.0.1",
    "glob": "^11.0.3",
    "gulp-imagemin": "^9.1.0",
    "gulp-rename": "^2.1.0",
    "gulp-standard": "^14.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^30.1.3",
    "jsdoc-to-markdown": "^9.1.2",
    "merge2": "^1.4.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "engines": {
    "node": ">=10.2.1"
  },
  "files": [
    "dist"
  ]
}
